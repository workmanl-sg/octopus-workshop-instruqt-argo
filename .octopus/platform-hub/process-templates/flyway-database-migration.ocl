        Octopus.ControlType = "WorkerPool"
    }
    help_text = "Choose the worker you want to run this step on"
    label = "Worker Pool"
}

parameter "parameter.database.package" {
    display_settings = {
        Octopus.ControlType = "Package"
    }
    help_text = "Choose which database package you want to migrate "
    label = "Database Package"
}

parameter "parameter.database.name" {
    display_settings = {
        Octopus.ControlType = "SingleLineText"
    }
    help_text = "The name of the database you want to migrate"
    label = "Database Name"
}

parameter "parameter.database.user" {
    display_settings = {
        Octopus.ControlType = "SingleLineText"
    }
    help_text = "User of database connection string"
    label = "Database User"
}

parameter "parameter.database.password" {
    display_settings = {
        Octopus.ControlType = "Sensitive"
    }
    help_text = "Password of database service "
    label = "Database Password"
}

parameter "parameter.database.host" {
    display_settings = {
        Octopus.ControlType = "SingleLineText"
    }
    help_text = "Hostname of Database Server"
    label = "Database Hostname"
}

parameter "parameter.container.feed" {
    display_settings = {
        Octopus.ControlType = "Feed"
    }
    help_text = ""
    label = "Container Feed"
}

parameter "parameter.package.path" {
    display_settings = {
        Octopus.ControlType = "SingleLineText"
    }
